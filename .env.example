# HPCSA Search App Environment Variables (Example)
# Copy this file to .env and update the values as needed

# HPCSA API Configuration
HPCSA_API_URL=https://hpcsaonline.custhelp.com/cc/ReportController/getDataFromRnow

# Playwright Browser Configuration (fallback for single search)
PLAYWRIGHT_TIMEOUT=30000

# Batch Processing Configuration
# BATCH_SIZE: Number of registrations to process concurrently
BATCH_SIZE=20
# MAX_CONCURRENT_REQUESTS: Max parallel API requests
MAX_CONCURRENT_REQUESTS=10
# REQUEST_DELAY_MS: Delay between batches (ms)
REQUEST_DELAY_MS=100
# REQUEST_TIMEOUT_MS: Timeout for each API request (ms)
REQUEST_TIMEOUT_MS=10000
# MAX_RETRIES: Max retry attempts for failed requests
MAX_RETRIES=3
